$("#sortable").html('<%=j render 'admin/partial/sort' %>');

id="#<%=@sort[0]%>_title";

if ($(id).attr('title')=='up'){

	<% s="#{@sort[0]},DESC" %>
$(id).attr("href","<%= admin_administrator_url(s) %>");
$(id).attr('title','down');
}else
{
<% s="#{@sort[0]},ASC" %>
$(id).attr("href","<%= admin_administrator_url(s) %>");
$(id).attr('title','up');
}

