<% @users.each do |key| %>

<li class="ui-state-default">


<span style="float:left;display:block;width:25%;padding:0 10px 0 10px; ">
 <%= link_to(key[:email], edit_admin_administrator_url(key[:id]), remote: true,style:"color: #337ab7;") %>
</span>
<span style="float:left;display:block;width:25%;padding:0 10px 0 10px; ">
<%= key[:last_name] %>
</span>
<span style="float:right;display:block;width:25%;text-align:center;padding:0 10px 0 10px;">
<%= link_to('X', admin_administrator_url(key[:id]), method: :delete, remote: true,style:"color: #337ab7;") %>
</span>
<span>

<%= key[:first_name] %>
</span>


<% end %>