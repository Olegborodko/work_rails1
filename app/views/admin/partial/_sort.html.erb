<%= javascript_include_tag "sortable" %>

<% if !@sort
	@sort=[]
  @sort[0]='position'
  @sort[1]='ASC'
	end %>

<% @users.each do |key| %>

<li class="ui-state-default" id="li_<%= key[:id] %>_<%= key[:position] %>_<%= @sort[0] %>_<%= @sort[1] %>">


<span class='name'>
 <%= link_to(key[:email], edit_admin_administrator_url(key[:id]), remote: true) %>
</span>

<span class='name'>
<%= key[:last_name] %>
</span>

<span class='name'>
<%= key[:first_name] %>
</span>

<span class='name'>
<%= link_to("delete element #{key[:position]}", admin_administrator_url(key[:id]), method: :delete, remote: true) %>
</span>



</li>
<% end %>

<br>
<%= ajax_will_paginate @users, :params => { :my_excluded_param => nil } %>
<%#= will_paginate @users %>

