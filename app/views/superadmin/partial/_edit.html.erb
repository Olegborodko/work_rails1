<%= form_for @user, url: superadmin_sadmin_url  do |f| %>

  <p>

    <%= f.label :email %><br />
    <%= f.text_field :email  %>
    </p>
  <p>
    <%= f.label :first_name %><br />
    <%= f.text_field :first_name %>
    </p>

  <p>
    <%= f.label :last_name %><br />
    <%= f.text_field :last_name %>
    </p>

  <p>
    <%= f.label :information %><br />
    <%= f.text_area :information, size: "30x3" %></p>


   <%= f.submit class:"btn btn-primary" %>

   <%= link_to "Cancel", superadmin_sadmin_index_url, class:"btn btn-primary" %>

<% end %>